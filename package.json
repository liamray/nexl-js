{
  "name": "nexl",
  "version": "3.1.0",
  "description": "nexl is a configuration storage where configuration stored in native JavaScript files, exposed via REST and accessed by special query language",
  "keywords": [
    "nexl",
    "nexl-js",
    "nexl-engine",
    "server",
    "rest",
    "web",
    "service",
    "http",
    "centralized",
    "config",
    "configuration",
    "storage",
    "data",
    "external",
    "javascript",
    "js",
    "expression",
    "scripting",
    "language",
    "ide",
    "editor",
    "devop",
    "devops"
  ],
  "license": "Apache-2.0",
  "author": {
    "name": "Liam Ray",
    "email": "info@nexl-js.com"
  },
  "homepage": "http://www.nexl-js.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/liamray/nexl-js.git"
  },
  "scripts": {
    "nexl": "node ./bin/nexl",
    "postinstall": "node -e \"require('print-message')(['Welcome to nexl !', 'If you have installed nexl globally with -g flag, type nexl to start a server', 'For other installations type npm run nexl', 'Visit http://www.nexl-js.com for more information']);\""
  },
  "bin": {
    "nexl": "bin/nexl"
  },
  "files": [
    "bin/nexl",
    "backend",
    "site"
  ],
  "engines": {
    "node": ">=6.0"
  },
  "dependencies": {
    "base-64": "^0.1.0",
